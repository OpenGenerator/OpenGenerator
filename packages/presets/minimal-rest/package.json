{
  "name": "@opengenerator/preset-minimal-rest",
  "version": "1.0.0",
  "description": "Minimal REST preset for OpenGenerator",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": { ".": { "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" }, "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" } } },
  "files": ["dist"],
  "scripts": { "build": "tsup", "dev": "tsup --watch", "test": "vitest run", "typecheck": "tsc --noEmit", "clean": "rimraf dist" },
  "dependencies": {
    "@opengenerator/core": "workspace:*",
    "@opengenerator/parser-json-schema": "workspace:*",
    "@opengenerator/gen-rest": "workspace:*",
    "@opengenerator/adapter-standalone": "workspace:*",
    "@opengenerator/auth-apikey": "workspace:*"
  },
  "devDependencies": { "@types/node": "^20.11.0", "rimraf": "^5.0.5", "tsup": "^8.0.1", "typescript": "^5.3.3", "vitest": "^1.2.0" },
  "keywords": ["opengenerator", "preset", "minimal", "rest"],
  "author": "Ersin KOC",
  "license": "MIT"
}

{
  "name": "@opengenerator/db-drizzle",
  "version": "1.0.0",
  "description": "Drizzle ORM database adapter for OpenGenerator",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": { ".": { "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" }, "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" } } },
  "files": ["dist"],
  "scripts": { "build": "tsup", "dev": "tsup --watch", "test": "vitest run", "typecheck": "tsc --noEmit", "clean": "rimraf dist" },
  "dependencies": { "@opengenerator/core": "workspace:*" },
  "peerDependencies": { "drizzle-orm": "^0.29.0" },
  "devDependencies": { "@types/node": "^20.11.0", "drizzle-orm": "^0.29.0", "rimraf": "^5.0.5", "tsup": "^8.0.1", "typescript": "^5.3.3", "vitest": "^1.2.0" },
  "keywords": ["opengenerator", "database", "drizzle", "orm"],
  "author": "Ersin KOC",
  "license": "MIT"
}

{
  "name": "opengenerator",
  "version": "1.0.0",
  "description": "Schema-to-API code generator - Transform your schemas into production-ready APIs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "opengenerator": "./dist/cli.js",
    "og": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": { "types": "./dist/index.d.ts", "default": "./dist/index.js" },
      "require": { "types": "./dist/index.d.cts", "default": "./dist/index.cjs" }
    },
    "./core": { "import": "@opengenerator/core", "require": "@opengenerator/core" },
    "./cli": { "import": "@opengenerator/cli", "require": "@opengenerator/cli" }
  },
  "files": ["dist"],
  "scripts": { "build": "tsup", "dev": "tsup --watch", "test": "vitest run", "typecheck": "tsc --noEmit", "clean": "rimraf dist" },
  "dependencies": {
    "@opengenerator/core": "workspace:*",
    "@opengenerator/cli": "workspace:*",
    "@opengenerator/parser-prisma": "workspace:*",
    "@opengenerator/parser-openapi": "workspace:*",
    "@opengenerator/parser-json-schema": "workspace:*",
    "@opengenerator/parser-zod": "workspace:*",
    "@opengenerator/parser-typebox": "workspace:*",
    "@opengenerator/parser-vld": "workspace:*",
    "@opengenerator/gen-rest": "workspace:*",
    "@opengenerator/gen-graphql": "workspace:*",
    "@opengenerator/gen-trpc": "workspace:*",
    "@opengenerator/adapter-express": "workspace:*",
    "@opengenerator/adapter-fastify": "workspace:*",
    "@opengenerator/adapter-hono": "workspace:*",
    "@opengenerator/adapter-koa": "workspace:*",
    "@opengenerator/adapter-standalone": "workspace:*",
    "@opengenerator/auth-jwt": "workspace:*",
    "@opengenerator/auth-oauth": "workspace:*",
    "@opengenerator/auth-session": "workspace:*",
    "@opengenerator/auth-apikey": "workspace:*",
    "@opengenerator/auth-magic-link": "workspace:*",
    "@opengenerator/auth-passkey": "workspace:*",
    "@opengenerator/db-prisma": "workspace:*",
    "@opengenerator/db-drizzle": "workspace:*",
    "@opengenerator/db-kysely": "workspace:*",
    "@opengenerator/db-typeorm": "workspace:*",
    "@opengenerator/db-mongoose": "workspace:*",
    "@opengenerator/db-raw-sql": "workspace:*",
    "@opengenerator/deploy-docker": "workspace:*",
    "@opengenerator/deploy-vercel": "workspace:*",
    "@opengenerator/deploy-railway": "workspace:*",
    "@opengenerator/deploy-fly": "workspace:*",
    "@opengenerator/deploy-lambda": "workspace:*",
    "@opengenerator/deploy-kubernetes": "workspace:*",
    "@opengenerator/preset-fullstack-prisma": "workspace:*",
    "@opengenerator/preset-serverless-drizzle": "workspace:*",
    "@opengenerator/preset-graphql-apollo": "workspace:*",
    "@opengenerator/preset-minimal-rest": "workspace:*",
    "@opengenerator/preset-enterprise": "workspace:*"
  },
  "devDependencies": { "@types/node": "^20.11.0", "rimraf": "^5.0.5", "tsup": "^8.0.1", "typescript": "^5.3.3", "vitest": "^1.2.0" },
  "keywords": ["opengenerator", "code-generator", "api", "rest", "graphql", "trpc", "prisma", "schema", "typescript"],
  "author": "Ersin KOC",
  "license": "MIT",
  "repository": { "type": "git", "url": "https://github.com/ersinkoc/opengenerator.git" },
  "bugs": { "url": "https://github.com/ersinkoc/opengenerator/issues" },
  "homepage": "https://opengenerator.dev"
}
